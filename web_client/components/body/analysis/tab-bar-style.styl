
.analysis-tab-bar
  min-height: 65px
  overflow: auto
  text-align: center
  white-space: nowrap
  border: 1px solid lightgray
  border-radius: 3px

.analysis-tab
  display: inline-block
  margin: 0px
  margin-top: 15px
  padding: 5px
  border: 2px solid black
  border-radius: 10px
  cursor: pointer

.analysis-tab.active
  color: white
  background: black
  cursor: context-menu

.analysis-tab.disabled
  color: #a5a5a5
  background: #eaeaea
  border-color: #a5a5a5
  cursor: no-drop

.analysis-tab-divider
  display: inline-block
  margin: 0px
  margin-top: 15px
  padding: 3px
  border: none
  border-top: 1px solid black
  border-radius: 10px
  min-width: 20px
  height: 0px

