  "dependencies": {
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "bluebird": "^3.4.6",
    "d3": "^4.2.6",
    "d3-array": "^1.0.2",
    "d3-scale": "^1.0.4",
    "d3-shape": "^1.0.4",
    "lodash": "^4.16.2",
    "raw-loader": "^0.5.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-sortable": "^1.2.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "urijs": "^1.18.4"
  },
  "scripts": {
    "lint": "npm run -s lint:server && npm run -s lint:client",
    "lint:client": "npm run -s lint:client:eslint && npm run -s lint:client:semi",
    "lint:client:semi": "semistandard --verbose --color | snazzy --color",
    "lint:client:eslint": "eslint --color -c .eslintrc.json --ext js --ext jsx web_client",
    "lint:server": "npm run -s lint:server:flake8",
    "lint:server:flake8": "python -m flake8 --config=.flake8.cfg server",
    "test:frontend": "babel-tape-runner 'web_client/tests/**/*.js*' | tap-spec"
  },
  "devDependencies": {
    "babel-tape-runner": "^2.0.1",
    "eslint": "^3.19.0",
